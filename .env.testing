APP_NAME="Insider SMS API"
APP_ENV=testing
APP_KEY=base64:your-test-key-here
APP_DEBUG=true
APP_URL=http://localhost

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=sqlite
DB_DATABASE=testing.sqlite

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Queue Configuration
QUEUE_CONNECTION=sync

# Cache Configuration
CACHE_DRIVER=array
SESSION_DRIVER=array
SESSION_LIFETIME=120

# Mail Configuration (Optional)
MAIL_MAILER=array
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="noreply@insider.com"
MAIL_FROM_NAME="${APP_NAME}"

# SMS Service Configuration
SMS_WEBHOOK_URL=https://webhook.site/23a9b629-861b-4a07-9ba6-97eecb2748ce
SMS_API_KEY=your-api-key
